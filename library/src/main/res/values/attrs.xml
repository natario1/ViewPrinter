            <enum name="NA_INDEX_3X5" value="35"/>
            <enum name="NA_INDEX_4X6" value="36"/>
            <enum name="NA_INDEX_5X8" value="37"/>
            <enum name="NA_JUNIOR_LEGAL" value="38"/>
            <enum name="NA_LEDGER" value="39"/>
            <enum name="NA_LEGAL" value="40"/>
